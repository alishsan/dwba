(defmacro infix
  "Use this macro when you pine for the notation of your childhood"
  [infixed]
  (list (second infixed) (first infixed) (last infixed)))
(defmacro infix
  [infixed1 inf2 inf 3]
))
(defmacro infix
  [infixed1 inf2 inf3]
  (list inf2 infix1 inf3))
(defmacro infix
  [inf1 inf2 inf3]
  (list inf2 inf1 inf3))
(infix 2 + 3)
(defmacro infix-2
  [[operand1 op operand2]]
  (list op operand1 operand2))
(infix-2 2 + 3)
(infix-2 (2 + 3))
exit
